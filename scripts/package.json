{
  "name": "andrewzc-photo-tools",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "description": "Utilities for image ingest, thumbnails, JSON updates, and S3 upload.",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "images": "node images.mjs",
    "images:open-all": "node images.mjs --open-all-tn",
    "upload": "node upload.mjs",
    "upload:dry": "node upload.mjs --dry-run",
    "upload:force": "node upload.mjs --force"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "sharp": "^0.33.0"
  }
}
